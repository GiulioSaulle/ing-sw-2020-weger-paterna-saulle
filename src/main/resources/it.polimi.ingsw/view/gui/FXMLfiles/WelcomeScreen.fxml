   <ImageView>
      <image>
         <Image url="@/it.polimi.ingsw/view/gui/Image/Scene/WelcomeBoard.png"/>
      </image>
      <effect>
         <Blend/>
      </effect>
      <viewport>
         <Rectangle2D/>
      </viewport>
   </ImageView>
   <Button fx:id="PlayButton" layoutX="200.0" layoutY="550.0" mnemonicParsing="false" styleClass="play"
           stylesheets="@buttons-style.css"/>
   <Button fx:id="HelpButton" layoutX="650.0" layoutY="550.0" mnemonicParsing="false" onAction="#HandleHelpButton"
           prefHeight="0.0" prefWidth="16.0" styleClass="help" stylesheets="@buttons-style.css"/>
</AnchorPane>
